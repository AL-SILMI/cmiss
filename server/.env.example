# Backend Environment Variables (Server)
# Copy this to .env file and update with your actual values

# Database Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/cmis?retryWrites=true&w=majority

# Authentication
JWT_SECRET=your-very-strong-secret-key-here-minimum-32-characters

# CORS Configuration
CORS_ORIGIN=https://your-frontend.vercel.app

# Server Port (Vercel will handle this automatically)
PORT=5000

# Node Environment
NODE_ENV=production